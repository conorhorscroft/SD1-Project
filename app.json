{
  "expo": {
    "name": "uni-project",
    "slug": "uni-project",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/favicon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.uni-project",
      "infoPlist": {
        "NSHealthUpdateUsageDescription": "We need access to your health data to update and sync your health statistics.",
        "NSHealthShareUsageDescription": "We need access to your health data to read your activity and health information."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/favicon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.uniproject"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "react-native-health",
      "react-native-health-connect"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
